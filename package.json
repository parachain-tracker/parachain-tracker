{
  "name": "parachain-tracker",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "run-p build:ssr",
    "build:client": "ng build --prod --progress=false",
    "build:api": "ng build api --prod --progress=false",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps --base=origin/develop",
    "affected:libs": "nx affected:libs --base=origin/develop",
    "affected:build": "nx affected:build --base=origin/develop",
    "affected:e2e": "nx affected:e2e --base=origin/develop",
    "affected:test": "nx affected:test --base=origin/develop",
    "affected:lint": "nx affected:lint --base=origin/develop",
    "affected:dep-graph": "nx affected:dep-graph --base=origin/develop",
    "affected": "nx affected --base=origin/develop",
    "format": "nx format:write --base=origin/develop",
    "format:all": "nx format:write --all",
    "format:write": "nx format:write --base=origin/develop",
    "format:check": "nx format:check --base=origin/develop",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "db": "./.bin/db",
    "db:init": "./.bin/db migration:generate -n CreateDatabase && ./.bin/db schema:sync",
    "db:migration:generate": "./.bin/db migration:generate -n MigrateDatabase",
    "db:migration:run": "./.bin/db migration:run",
    "db:migration:revert": "./.bin/db migration:revert",
    "db:schema:sync": "./.bin/db schema:sync",
    "db:schema:log": "./.bin/db schema:log",
    "db:schema:drop": "./.bin/db schema:drop",
    "db:seed": "./.bin/seed-db",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node local.js",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run parachain-tracker:server:production",
    "server": "node local.js",
    "build:prod": "npm run build:ssr",
    "serve:prerender": "node static.js",
    "build:prerender": "npm run build:prod && node dist/prerender.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.1",
    "@angular/cdk": "8.1.2",
    "@angular/common": "8.2.1",
    "@angular/compiler": "8.2.1",
    "@angular/core": "8.2.1",
    "@angular/forms": "8.2.1",
    "@angular/platform-browser": "8.2.1",
    "@angular/platform-browser-dynamic": "8.2.1",
    "@angular/platform-server": "8.2.1",
    "@angular/router": "8.2.1",
    "@fortawesome/fontawesome-free": "5.10.1",
    "@nestjs/common": "6.5.3",
    "@nestjs/core": "6.5.3",
    "@nestjs/platform-express": "6.5.3",
    "@nestjs/typeorm": "6.1.3",
    "@ng-toolkit/universal": "7.1.2",
    "@nguniversal/common": "8.1.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@nrwl/angular": "8.4.3",
    "@types/faker": "4.1.5",
    "@types/flickity": "2.2.0",
    "chart.js": "2.8.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.9.1",
    "core-js": "3.1.4",
    "domino": "2.1.3",
    "express": "4.17.1",
    "flickity": "2.2.1",
    "googleapis": "42.0.0",
    "http-proxy-middleware": "0.19.1",
    "ng2-charts": "2.3.0",
    "preboot": "7.0.0",
    "parse-link-header": "1.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "sqlite3": "4.0.9",
    "typeorm": "0.2.18",
    "typeorm-seeding": "1.0.0-rc.3",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.802.0",
    "@angular/cli": "8.2.1",
    "@angular/compiler-cli": "8.2.1",
    "@angular/language-service": "8.2.1",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@nestjs/schematics": "6.4.1",
    "@nestjs/testing": "6.5.3",
    "@nrwl/cypress": "8.4.3",
    "@nrwl/jest": "8.4.3",
    "@nrwl/nest": "8.4.3",
    "@nrwl/node": "8.4.3",
    "@nrwl/workspace": "8.4.3",
    "@types/dotenv": "6.1.1",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@types/sqlite3": "3.1.5",
    "codelyzer": "5.1.0",
    "conventional-changelog-cli": "2.0.23",
    "cypress": "3.4.1",
    "cz-conventional-changelog": "3.0.2",
    "dotenv": "8.0.0",
    "eslint": "6.1.0",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "jest-preset-angular": "7.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "ts-jest": "24.0.2",
    "ts-loader": "5.2.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.4.5",
    "webpack-cli": "3.3.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:check",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
